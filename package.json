{
  "name": "worldstore-agent",
  "version": "1.0.0",
  "description": "example agent implementations for Crossmint's worldstore APIs.",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "start": "pnpm -F agent run start",
    "dev": "pnpm -r run dev",
    "dev:server": "pnpm -F server run dev",
    "dev:agent": "pnpm -F agent run dev",
    "start:server": "pnpm -F server run start",
    "start:agent": "pnpm -F agent run start",
    "lint": "pnpm -r run lint",
    "lint:check": "pnpm -r run lint:check",
    "format": "pnpm -r run format",
    "format:check": "pnpm -r run format:check",
    "type:check": "pnpm -r run type:check",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "test": "pnpm -r run test"
  },
  "workspaces": [
    "agent",
    "server"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "x402",
    "crossmint",
    "xmtp",
    "crypto",
    "payments",
    "blockchain"
  ],
  "author": "Crossmint",
  "license": "MIT",
  "packageManager": "pnpm@9.15.2+sha1.0ffb02f94047016ec7e088030337455b4c94bb34"
}
